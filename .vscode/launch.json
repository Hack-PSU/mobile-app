  "version": "0.2.0",
  "configurations": [
    {
      "name": "hackpsu dev debug",
      "program": "lib/main_dev.dart",
      "request": "launch",
      "type": "dart",
      "args": [
        "--flavor",
        "dev" //development or staging or production
      ]
    },
    {
      "name": "hackpsu dev release",
      "program": "lib/main_dev.dart",
      "request": "launch",
      "type": "dart",
      "args": [
        "--release",
        "--flavor",
        "dev"
      ]
    },
    {
      "name": "hackpsu dev profile",
      "program": "lib/main_dev.dart",
      "request": "launch",
      "type": "dart",
      "args": [
        "--profile",
        "--flavor",
        "dev"
      ]
    },
    {
      "name": "hackpsu prod debug",
      "program": "lib/main.dart",
      "request": "launch",
      "type": "dart",
      "args": [
        "--flavor",
        "prod"
      ]
    },
    {
      "name": "hackpsu prod release",
      "program": "lib/main.dart",
      "request": "launch",
      "type": "dart",
      "args": [
        "--release",
        "--flavor",
        "prod"
      ]
    },
    {
      "name": "hackpsu prod profile",
      "program": "lib/main.dart",
      "request": "launch",
      "type": "dart",
      "args": [
        "--profile",
        "--flavor",
        "prod"
      ]
    },
  ]
}
